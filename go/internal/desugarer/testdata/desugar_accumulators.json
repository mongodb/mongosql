        }
      }
    ],
    "expected": [
      {
        "$group": {
          "_id": null,
          "acc": {
            "$addToSet": "$a"
          }
        }
      },
      {
        "$project": {
          "acc": {
            "$reduce": {
              "input": "$acc",
              "initialValue": 0,
              "in": {
                "$add": [
                  "$$value",
                  {
                    "$cond": [
                      {
                        "$in": [
                          {
                            "$type": "$$this"
                          },
                          [
                            "missing",
                            "null"
                          ]
                        ]
                      },
                      0,
                      1
                    ]
                  }
                ]
              }
            }
          }
        }
      }
    ]
  },
