    ],
    "expected": [
      {
        "$group": {
          "_id": null,
          "acc": {
            "$sum": {
              "$cond": [
                {
                  "$in": [
                    {
                      "$type": "$a"
                    },
                    [
                      "missing",
                      "null"
                    ]
                  ]
                },
                0,
                1
              ]
            }
          }
        }
      }
    ]
  },
  {
    "name": "desugar distinct $sqlCount",
    "input": [
      {
        "$group": {
          "_id": null,
          "acc": {
            "$sqlCount": {
              "var": "$a",
