  {
    "name": "desugar mix of distinct $sql, non-distinct $sql, and non-$sql accumulators",
    "input": [
      {
        "$group": {
          "_id": null,
          "nonDistinct": {
            "$sqlAvg": {
              "var": "$a",
              "distinct": false
            }
          },
          "distinct": {
            "$sqlLast": {
              "var": "$b",
              "distinct": true
            }
          },
          "nonSQL": {
            "$first": "$c"
          }
        }
      }
    ],
    "expected": [
      {
        "$group": {
          "_id": null,
          "nonDistinct": {
            "$avg": "$a"
          },
          "distinct": {
            "$addToSet": "$b"
          },
          "nonSQL": {
            "$first": "$c"
          }
        }
      },
      {
        "$project": {
          "nonDistinct": 1,
          "distinct": {
            "$last": "$distinct"
          },
          "nonSQL": 1
        }
      }
    ]
